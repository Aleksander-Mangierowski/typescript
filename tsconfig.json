{
    "compilerOptions": {
        "target": "es6",
        "module": "es6",
        "moduleResolution": "node",
        "sourceMap": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "pretty": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}